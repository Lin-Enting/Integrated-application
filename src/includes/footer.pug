footer.l-footer 
  .o-gotop
    .o-gotop__arrow
    i.fas.fa-long-arrow-alt-up
    .o-gotop__text TOP
      
  .l-footer__date-mask(uk-parallax="bgy: 100")
    img.l-footer__img-date.js-lazy(data-src="img/footer/footer-date.svg", alt="大港開唱資訊")

  .l-footer__sitemap
    ul.l-footer__btn-box
      li.l-footer__btn-wrap
        a.l-footer__btn(href="##")
          .l-footer__text-btn.--en.u-text-footer INFO
          .l-footer__text-btn.--zh.u-text-footer 相關資訊
      li.l-footer__btn-wrap
        a.l-footer__btn(href="##")
          .l-footer__text-btn.--en.u-text-footer NEWS
          .l-footer__text-btn.--zh.u-text-footer 最新消息
      li.l-footer__btn-wrap
        a.l-footer__btn(href="##")
          .l-footer__text-btn.--en.u-text-footer ARTISTS
          .l-footer__text-btn.--zh.u-text-footer 藝人名單
      li.l-footer__btn-wrap
        a.l-footer__btn(href="##")
          .l-footer__text-btn.--en.u-text-footer TIMETABLE
          .l-footer__text-btn.--zh.u-text-footer 節目表
      li.l-footer__btn-wrap
        a.l-footer__btn(href="##")
          .l-footer__text-btn.--en.u-text-footer TICKET
          .l-footer__text-btn.--zh.u-text-footer 購票資訊
      li.l-footer__btn-wrap
        a.l-footer__btn(href="map.html")
          .l-footer__text-btn.--en.u-text-footer MAP
          .l-footer__text-btn.--zh.u-text-footer 地圖導覽

  .l-footer__link-box
    a.l-footer__link-wrap(href="https://www.facebook.com//megaportfest/" target="_blank")
      i.fab.fa-facebook.l-footer__link
    a.l-footer__link-wrap(href="https://www.instagram.com/megaportfest/" target="_blank") 
      i.fab.fa-instagram.l-footer__link
    a.l-footer__link-wrap(href="https://www.youtube.com/channel/UC7xWU14_GGYKVqoSLg1M8EQ/feed" target="_blank") 
      i.fab.fa-youtube.l-footer__link

